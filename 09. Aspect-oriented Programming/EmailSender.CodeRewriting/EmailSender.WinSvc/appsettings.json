{
  "Serilog": {
    "Using": [ "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "C:\\Logs\\EmailSender.WinSvc\\{Date}.log",
          "retainedFileCountLimit": 180
        }
      }
    ]
  }
}
