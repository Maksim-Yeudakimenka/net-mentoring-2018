  </appSettings>
  <connectionStrings>
    <add name="emailMessaging" connectionString="server=(localdb)\MSSQLLocalDB;database=Email_Messaging;Trusted_Connection=Yes;" providerName="System.Data.SqlClient" />
  </connectionStrings>
